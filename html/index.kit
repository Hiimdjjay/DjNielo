                <form action="mail.php" method="post" class="contact__form">
                    <div class="contact__box">
                        <label class="contact__box-label" for="name">Imię i nazwisko:</label>
                        <input class="contact__box-input js" type="text" id="name" name="name"
                            placeholder="Podaj imię oraz nazwisko" required>
                        <p class="contact__box-error">Nieprawidłowe imię</p>
                    </div>

                    <div class="contact__box ">
                        <label class="contact__box-label" for="email">E-mail:</label>
                        <input class="contact__box-input  js" type="email" id="email" name="email"
                            placeholder="Podaj e-mail" required>
                        <p class="contact__box-error ">Nieprawidłowe imię</p>
                    </div>

                    <div class="contact__box">
                        <label class="contact__box-label" for="phone">Numer telefonu (Opcjonalnie):</label>
                        <input class="contact__box-input js" type="tel" id="phone" name="phone"
                            placeholder="Podaj numer telefonu">
                        <p class="contact__box-error">Nieprawidłowe imię</p>
                    </div>

                    <div class="contact__box">
                        <label class="contact__box-label" for="date">Data wydarzenia:</label>
                        <input class="contact__box-input js" type="date" name="date" id="date"
                            placeholder="Podaj datę wydarzenia" required>
                        <p class="contact__box-error">Nieprawidłowe imię</p>
                    </div>

                    <div class="contact__box">
                        <label class="contact__box-label" for="message">Wiadomość:</label>
                        <textarea class="contact__box-input textarea" id="message" name="message"
                            placeholder="Podaj treść wiadomości" required></textarea>
                        <p class="contact__box-error">Nieprawidłowe imię</p>
                    </div>
                    <div class="contact__box-buttons">
                        <button class="contact__button contact__button--clear">Wyczyść</button>
                        <button class="contact__button contact__button--submit">Wyślij</button>
                    </div>

